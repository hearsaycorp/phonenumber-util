{
  "name": "@yext/phonenumber-util",
  "version": "0.4.0",
  "author": "bajohnson@hearsaycorp.com",
  "license": "BSD-3-Clause",
  "description": "Utility for extracting and validating phone numbers",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "src",
  "exports": {
    ".": "./src/base.js",
    "./geo": "./src/geo.js"
  },
  "types": "src/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hearsaycorp/phonenumber-util.git"
  },
  "scripts": {
    "lint": "eslint ./src/*.js ./src/**/*.js",
    "format": "prettier --write ./src/*.js ./src/**/*.js",
    "test": "TZ=America/Los_Angeles vitest",
    "build": "node -e \"console.log('No build script for vanilla JS')\"",
    "prepare": "husky",
    "make-badges": "istanbul-badges-readme",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "release": "npm run test && npm run lint && git push && git push --tags"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@vitest/coverage-v8": "^4.0.2",
    "eslint": "^9.38.0",
    "generate-license-file": "^4.1.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "istanbul-badges-readme": "^1.9.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "vitest": "^4.0.2"
  },
  "eslintConfig": {},
  "lint-staged": {
    "**/*.js": [
      "npm run lint"
    ],
    "**/*.{js,json,md}": [
      "npm run format"
    ]
  }
}
